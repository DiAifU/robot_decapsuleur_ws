		<origin rpy="0 0 0" xyz="0 0 -0.15"/>
	</joint>
	<joint name="joint2" type="continuous">
		<parent link="dyna1"/>
		<child link="rot"/>
		<origin rpy="0 0 0" xyz="0 0 0.185"/>
